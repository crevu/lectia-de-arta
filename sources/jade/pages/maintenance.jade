extends  ../_skeleton
//- Includes
include ../elements/_ui-timers
include ../elements/_widget-form-subscribe-type-2
//- Title
block title
  title= cfg.nav.pages.submenu.pages.submenu.maintenance.title
//- Page
block page
  // Page Header
  header.page-head
  // Page Content
  main.page-content.section-cover.section-404.section-30.shell
    div
      a(href="./")
        img(src="images/logo-170x172.png" width=100 height=100 alt="")
    div.section-50
      .offset-top-10
        h2.text-bold We are currently working on. #[br.veil.reveal-lg-inline-block] This page will appear in
      .offset-top-15.offset-lg-top-30

      .offset-top-30.offset-lg-top-60
        .offset-top-66(style="max-width: 770px; margin-left: auto; margin-right: auto")
          .DateCountdown(data-date='2017-07-1 01:00:00')
      .offset-top-30.offset-lg-top-70
        p Stay tuned and subscribe to our newsletter
      .offset-top-15
        .range.range-xs-center
          .cell-sm-6
            +widget-form-subscribe-type-2("form-subscribe-forms-page")
    div
      p
        | &copy; #[span(id="copyright-year")] All Rights Reserved Terms of Use and #[a(href="privacy.html") Privacy Policy]