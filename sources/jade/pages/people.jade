extends  ../_skeleton
//- Includes
include ../elements/_ui-breadcrumb-classic
//- Title
block title
  title= cfg.nav.pages.submenu.about.submenu.people.title
//- Page
block page
  // Page Header
  header.page-head
    //- RD Navbar
    +rd-navbar-corporate(
    'rd-navbar-default',
    {
      "data-lg-auto-height": "true",
      "data-md-layout": 'rd-navbar-static',
      "data-lg-layout": 'rd-navbar-static',
      "data-lg-stick-up": 'true'
    },
    cfg.brand.logoCorporateNavbar,
    cfg.nav.pages,
    cfg.nav
    )
  +breadcrumb-classic(
  cfg.nav.pages.submenu.about.submenu.people.title,
  {
    0: {
      "title": cfg.nav.home.title,
      "href": cfg.nav.home.href
    },
    1: {
      "title": cfg.nav.pages.title,
      "href": cfg.nav.pages.href
    },
    2: {
      "title": cfg.nav.pages.submenu.about.title,
      "href": cfg.nav.pages.submenu.about.href
    },
    3: {
      "title": cfg.nav.pages.submenu.about.submenu.people.title,
      "href": cfg.nav.pages.submenu.about.submenu.people.href
    }
  })
  // Page Content
  main.page-content
    //4 Columns Layout
    section.section-70.section-md-114
      .shell
        h2.text-bold 4 Columns Layout

        .range.text-md-left.offset-top-60
          .cell-sm-6.cell-md-3
            img.img-responsive.reveal-inline-block.img-rounded(src="images/users/user-kathy-gibson-270x270.jpg" width=270 height=270 alt="")
            .offset-top-20
              h6.text-bold.text-primary
                a(href= cfg.nav.pages.submenu.about.submenu.teamMemberProfile.href) Kathy Gibson
            .offset-top-5
              p Preschool Director
          .cell-sm-6.cell-md-3.offset-top-30.offset-sm-top-0
            img.img-responsive.reveal-inline-block.img-rounded(src="images/users/user-julie-weaver-270x270.jpg" width=270 height=270 alt="")
            .offset-top-20
              h6.text-bold.text-primary
                a(href= cfg.nav.pages.submenu.about.submenu.teamMemberProfile.href) Julie Weaver
            .offset-top-5
