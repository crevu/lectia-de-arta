extends  ../_skeleton

//- Includes
include ../elements/_ui-tables
include ../elements/_ui-counter-type-2
include ../elements/_ui-rd-parallax
include ../elements/_widget-flickrfeed
include ../elements/_ui-rd-map
include ../elements/_ui-rd-video
include ../elements/_widget-form-subscribe
include ../elements/_ui-rd-navbar-transparent

//- Title
block title
  title= cfg.nav.home.title

//- Page
block page
  // Page Header
  header.page-head(style="position:absolute; left:0; right:0;top:0")
    //- RD Navbar
    +rd-navbar-transparent(
    'rd-navbar-transparent',
    {
      "data-lg-auto-height": "true",
      "data-auto-height": "false",
      "data-md-layout": 'rd-navbar-fixed',
      "data-lg-layout": 'rd-navbar-static',
      "data-lg-device-layout": 'rd-navbar-static',
      "data-lg-stick-up": 'true',
      "data-md-focus-on-hover": 'false'
    },
    cfg.brand.logoCorporateNavbar,
    cfg.nav.home,
    cfg.nav
    )

  // Page Content
  main.page-content
    //- Welcome
    section
      // Swiper
      .swiper-container.swiper-slider(data-height="100vh" data-loop="false" data-dragable="false" data-min-height="480px" data-slide-effect="true")
        .swiper-wrapper
          .swiper-slide(data-slide-bg='images/slide-01-1920x810.jpg' style="background-position: 80% center")
            .swiper-slide-caption.header-transparent-slide-caption
              .container
                .range.range-xs-center.range-lg-right.range-condensed
                  .cell-md-6.text-md-left.cell-xs-10
                    div(data-caption-animate="fadeInUp" data-caption-delay="100" data-caption-duration="1700")
                      h1.text-bold
                        | Joaca-te, Exploreaza si Invata.
                    .offset-top-20.offset-xs-top-40.offset-xl-top-60(data-caption-animate="fadeInUp" data-caption-delay="150" data-caption-duration="1700")
                      h5.text-regular.font-default Pe asta ne bazam in incercarea de a-i introduce pe cei mici in minunata lume a teatrului ...
                    .offset-top-20.offset-xl-top-40(data-caption-animate="fadeInUp" data-caption-delay="400" data-caption-duration="1700")
                      a(href= cfg.nav.pages.submenu.pages.submenu.loginRegister.href).btn.btn-madison.btn-ellipse Sign Up for Excursion
                      .inset-xs-left-30.reveal-lg-inline-block
                        a(href= cfg.nav.pages.submenu.programs.href).btn.btn-primary-transparent.btn-ellipse.veil.reveal-lg-inline-block Mai mult
          .swiper-slide(data-slide-bg='images/slide-02-1920x810.jpg' style="background-position: 80% center")
            .swiper-slide-caption.header-transparent-slide-caption
              .container
                .range.range-xs-center.range-lg-left.range-condensed
                  .cell-md-6.text-md-left.cell-xs-10
                    div(data-caption-animate="fadeInUp" data-caption-delay="100" data-caption-duration="1700")
                      h1.text-bold
                        | Locul perfect pentru dezvoltarea celor mici.
                    .offset-top-20.offset-xs-top-40.offset-xl-top-60(data-caption-animate="fadeInUp" data-caption-delay="150" data-caption-duration="1700")
                      h5.text-regular.font-default Any successful career starts with good education. At our learning center, your children will have deeper knowledge of all necessary subjects that will be especially useful for them.
                    .offset-top-20.offset-xl-top-40(data-caption-animate="fadeInUp" data-caption-delay="400" data-caption-duration="1700")
                      a(href= cfg.nav.pages.submenu.pages.submenu.loginRegister.href).btn.btn-ellipse.btn-madison Sign Up for Excursion
                      .inset-xs-left-30.reveal-lg-inline-block
                        a(href= cfg.nav.pages.submenu.programs.href).btn.btn-ellipse.btn-primary-transparent.veil.reveal-lg-inline-block Mai mult
          .swiper-slide(data-slide-bg='images/slide-03-1920x810.jpg' style="background-position: 80% center")
            .swiper-slide-caption.header-transparent-slide-caption
              .container
                .range.range-xs-center.range-lg-right.range-condensed
                  .cell-md-6.text-md-left.cell-xs-10
                    div(data-caption-animate="fadeInUp" data-caption-delay="100" data-caption-duration="1700")
                      h1.text-bold
                        | We Offer Different Kinds of Activities.
                    .offset-top-20.offset-xs-top-40.offset-xl-top-60(data-caption-animate="fadeInUp" data-caption-delay="150" data-caption-duration="1700")
                      h5.text-regular.font-default With us, your kid will enjoy plenty of different activities that will develop them and act as a perfect ground for further development. Explore the bright future of your child at Kids Tree Preschool!
                    .offset-top-20.offset-xl-top-40(data-caption-animate="fadeInUp" data-caption-delay="400" data-caption-duration="1700")
                      a(href= cfg.nav.pages.submenu.pages.submenu.loginRegister.href).btn.btn-ellipse.btn-madison Sign Up for Excursion
                      .inset-xs-left-30.reveal-lg-inline-block
                        a(href= cfg.nav.pages.submenu.programs.href).btn.btn-ellipse.btn-primary-transparent.veil.reveal-lg-inline-block Mai mult
        // Swiper Pagination
        .swiper-pagination


    // A Few Words About the University
    section.section-70.section-md-114.bg-index-body
      .shell
        .range.text-sm-left.range-sm-justify
          .cell-sm-7.view-animate.fadeInRightSm.delay-04
            .img-wrap-2
              figure
                span.icon.mdi.mdi-play-circle-outline(data-toggle='modal', data-target='#myModal')
                img.img-responsive.reveal-inline-block(src="images/home-01-620-350.jpg" width=620 height=350 alt="")
          .cell-sm-5.offset-top-50.offset-sm-top-0
            h2.home-headings-custom.text-bold.view-animate.fadeInLeftSm.delay-06 Despre noi
            .offset-top-35.offset-md-top-60.view-animate.fadeInLeftSm.delay-08
              p
                | We offer a play-based emergent curriculum complete with low teacher to child ratios. Come see our environment and how it is designed to promote exploration, adventure and imaginative play. We believe that children are active participants in their own learning. That’s why we encourage them to ask questions and explore the world around them.
            .offset-top-30.view-animate.fadeInLeftSm.delay-1
              a(href= cfg.nav.pages.submenu.about.submenu.history.href).btn.btn-ellipse.btn-icon.btn-icon-right.btn-default
                span.icon.fa-arrow-right
                span Mai mult

    // Counters
    section.text-center.bg-index-body
      .shell
        .range.range-xs-center.range-sm-left.offset-top-0
          .cell-xs-10.cell-sm-7.section-image-aside.section-image-aside-right
            .section-image-aside-img.veil.reveal-sm-block.view-animate.zoomInSmall.delay-04(style='background-image: url(images/home-01-846x1002.jpg)')
            .section-image-aside-body.section-70.section-md-114.inset-md-right-70.inset-lg-right-110
              h2.home-headings-custom.text-bold.view-animate.fadeInLeftSm.delay-04 Our Features

              .offset-top-35.offset-md-top-60.view-animate.fadeInLeftSm.delay-06.
                At Kids Tree, we see our aim in providing care and an early learning environment which is tailored to each individual child’s needs, in safe, secure, happy and relaxed surroundings. We are committed to your children’s success!
              .text-center.text-xs-left
                .range.range-xs-center.range-md-left.offset-top-65.counters
                  .cell-xs-6.view-animate.fadeInLeftSm.delay-08
                    +counter-type-2("text-madison mdi mdi-school", "Awards",{
                      "dataTo": "15",
                      "dataSpeed": "1300",
                      "preffix": ""
                    })
                  .cell-xs-6.offset-top-65.offset-xs-top-0.view-animate.fadeInLeftSm.delay-04
                    +counter-type-2("text-madison mdi mdi-wallet-travel","Certified Teachers", {
                      "dataTo": "30",
                      "dataSpeed": "1250",
                      "preffix": "+"
                    })
                  .cell-xs-6.offset-top-65.view-animate.fadeInLeftSm.delay-1
                    +counter-type-2("text-madison mdi mdi-domain","Featured Programs ",{
                      "dataTo": "10",
                      "dataSpeed": "500"
                    })
                  .cell-xs-6.offset-top-65.view-animate.fadeInLeftSm.delay-06
                    +counter-type-2("text-madison mdi mdi-account-multiple-outline","Children", {
                      "dataTo": "100",
                      "dataSpeed": "1500"
                    })

    // Testimonials
    section.context-dark.position-relative
      +rd-parallax({
        "backgroundSpeed": "0.2",
        "attrType":"media",
        "src":"images/parallax-01.jpg",
        "htmlSpeed": "0.05"
      })
        .shell.section-70.section-xl-100.section-xl-bottom-114
          .owl-carousel.owl-carousel-default.veil-xl-owl-dots.veil-owl-nav.reveal-xl-owl-nav(data-items="1", data-sm-items="2",  data-nav="true" data-dots="true" , data-margin="30", data-loop="true" data-nav-class='["owl-prev fa-angle-left", "owl-next fa-angle-right"]')
            div
              .quote-classic-boxed.text-left
                .quote-body
                  q Words truly can't express how grateful I am that a preschool like this exists. I thought that a place like this was a mythical unicorn that parents searched for, but never found.
                  .offset-top-30.text-left
                    .unit.unit-horizontal
                      .unit-left
                        img.img-responsive.reveal-inline-block.img-circle(src="images/users/user-debra-banks-230x230.jpg", width=80, height=80, alt="")
                      .unit-body
                        cite.font-accent Debra Banks
                        .offset-top-5
                          p.text-dark.text-italic Sales manager


            div
              .quote-classic-boxed.text-left
                .quote-body
                  q  I want to take this opportunity to thank you and your staff for all you have done for my daughter. I am in awe at how much Lauren has grown both emotionally and intellectually since last year.
                  .offset-top-30.text-left
                    .unit.unit-horizontal
                      .unit-left
                        img.img-responsive.reveal-inline-block.img-circle(src="images/users/user-steven-alvarez-230x230.jpg", width=80, height=80, alt="")
                      .unit-body
                        cite.font-accent Steven Alvarez
                        .offset-top-5
                          p.text-dark.text-italic Taxi driver
            div
              .quote-classic-boxed.text-left
                .quote-body
                  q Words truly can't express how grateful I am that a preschool like this exists. I thought that a place like this was a mythical unicorn that parents searched for, but never found.
                  .offset-top-30.text-left
                    .unit.unit-horizontal
                      .unit-left
                        img.img-responsive.reveal-inline-block.img-circle(src="images/users/user-debra-banks-230x230.jpg", width=80, height=80, alt="")
                      .unit-body
                        cite.font-accent Debra Banks
                        .offset-top-5
                          p.text-dark.text-italic Sales manager


            div
              .quote-classic-boxed.text-left
                .quote-body
                  q  I want to take this opportunity to thank you and your staff for all you have done for my daughter. I am in awe at how much Lauren has grown both emotionally and intellectually since last year.
                  .offset-top-30.text-left
                    .unit.unit-horizontal
                      .unit-left
                        img.img-responsive.reveal-inline-block.img-circle(src="images/users/user-steven-alvarez-230x230.jpg", width=80, height=80, alt="")
                      .unit-body
                        cite.font-accent Steven Alvarez
                        .offset-top-5
                          p.text-dark.text-italic Taxi driver

    // Gallery
    section
      .owl-carousel.flickr(data-items="1" data-xs-items="2" data-sm-items="3" data-lg-items='4' data-xl-items="6" data-nav="false" data-dots="false" data-mouse-drag="true" data-photo-swipe-gallery='' data-flickr-tags="tm-61183" data-stage-padding="0" data-xl-stage-padding="0")
        a.thumbnail-default.thumbnail-flickr(data-photo-swipe-item="" href="" data-image_c='href' data-size="320x320" data-type='flickr-item')
