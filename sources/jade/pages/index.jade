          img(width="320" height="320" data-title='alt', src='images/_blank.png', alt='', "data-image_z"= "src")
          span.icon.fa-search-plus
    // Contact info
    section.section-60
      .shell
        .range.range-xs-center.features-list
          .cell-sm-4
            .unit.unit-vertical.unit-responsive-md.section-45
              .unit-left
                span.icon.icon-contact-sm.text-madison.mdi.mdi-phone
              .unit-body
                a(href='callto:#').h6.text-regular 1-800-1234-567
          .cell-sm-4
            .unit.unit-vertical.unit-responsive-md.section-45
              .unit-left
                span.icon.icon-contact-sm.text-madison.mdi.mdi-map-marker
              .unit-body
                a(href='#').h6.text-regular 2130 Fulton Street San Diego, #[br.visible-lg-inline] CA 94117-1080 USA
          .cell-sm-4
            .unit.unit-vertical.unit-responsive-md.section-45
              .unit-left
                span.icon.icon-contact-sm.text-madison.mdi.mdi-email-open
              .unit-body
                a(href='mailto:#').h6.text-regular mail@demolink.org
